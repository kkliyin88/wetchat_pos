.title {
  color: #666;
  font-weight: 600;
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 25rpx;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  line-height: 3rem;
  align-items: center;
  border-top: 0.5rpx solid #f4f6ff;
  background: #fff;
  color: #000537;
  font-size: 24rpx;
}

.td {
  width: 40%;
  justify-content: center;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
}

.td0 {
  color: #666;
  font-weight: bold;
  background: #fff;
}

.td1 {
  color: #666;
  font-weight: 500;
  background: #fafbff;
}

.td2 {
  color: #666;
  background: #fff;
  font-weight: 500;
}

.td3 {
  color: #828bf0;
  font-weight: 500;
  background: #fafbff;
}

.header {
  color: #666;
  font-size: 30rpx;
  border-top: none;
}

.th {
  width: 40%;
  justify-content: center;
  display: flex;
  font-size: 30rpx;
  font-weight: bold;
  align-items: center;
}

image {
  height: 60rpx;
  width: 60rpx;
}
