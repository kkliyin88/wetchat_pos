/* pages/home/index.wxss */

page {
  overflow: hidden;
}

.title {
  text-align: center;
  font-size: 36rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  position: relative;
}

.goback {
  height: 35rpx;
  width: 35rpx;
  position: absolute;
  left: 20rpx;
}

.condition {
  height: 33rpx;
  line-height: 33rpx;
  margin: 30rpx 0;
  color: #fff;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-items: center;
  justify-content: space-between;
}

.condition .text {
  display: inline-block;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #fff;
  line-height: 33rpx;
  height: 34rpx;
  padding: 0 15rpx;
  margin: 0 22rpx;
  background: rgba(255, 255, 255, 0.25);
  border-radius: 17rpx;
}

.condition .chang_acount {
  background: #7783ef;
  line-height: 33rpx;
  height: 34rpx;
}

.title2 {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 40rpx;
  color: #fff;
  text-align: center;
}

.content_box {
  height: 120rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin-top: 70rpx;
}

.content {
  height: 120rpx;
  width: 30%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.content_box .content .text {
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  font-weight: 300;
  font-family: PingFangSC-Light, PingFang SC;
  line-height: 42rpx;
}

.container_content {
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 50rpx;
  color: #fff;
}

.container_content text {
  border-bottom: 1px solid #fff;
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 15rpx;
  display: inline-block;
}

.flex_box {
  display: flex;
  width: 100%;
  height: 135rpx;
  flex-direction: row;
  justify-content: space-around;
}

.flex_item {
  height: 135rpx;
  width: 292rpx;
  background: #fff;
  box-shadow: 0px 2rpx 8rpx 0px rgba(209, 212, 243, 1);
  border-radius: 18rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.flex_item text {
  display: block;
  height: 42rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000537;
  line-height: 42rpx;
}

.flex_item text.value {
  color: #7480ee;
}

page {
  background-color: #fff;
  position: relative;
  height: 100%;
}

.top {
  height: 45rpx;
  line-height: 45rpx;
  padding-left: 25rpx;
  color: #1e1e1e;
  margin-top: 40rpx;
  position: relative;
  z-index: 1000;
}

.top image {
  height: 45rpx;
  width: 40rpx;
  display: inline-block;
}

.top text {
  height: 45rpx;
  line-height: 45rpx;
  display: inline-block;
  margin: 0 10rpx;
  position: absolute;
}

.add_wrap {
  height: 150rpx;
  width: 100%;
  position: fixed;
  animation: move 0.3s 1;
  bottom:0;
  display: flex;
  justify-content: center;
}
@keyframes move{
   0% {
   bottom:-100rpx;
  }
  100% {
   bottom:0;
  }
}


.add_box {
  height: 240rpx;
  width: 240rpx;
  border: 1px solid #d1d4f3;
  border-radius: 50%;
  position: relative;
  vertical-align: center;
  text-align: center;
}

.add_box image {
  height: 118rpx;
  width: 118rpx;
  position: absolute;
  z-index: 200;
}

.add_box .sale {
  left: -50rpx;
}

.add_box .profit {
  right: -50rpx;
}

.add_box image.add {
  width: 58rpx;
  height: 58rpx;
  position: relative;
  margin-top: 70rpx;
}

.bottom_bg {
  height: 95rpx;
  width: 100%;
  position: fixed;
  z-index:-101;
  bottom: 0;
  background: url('data:image/png;base64,');
  background-size: cover;
}

.bottom_bg image {
  height: 95rpx;
  width: 100%;
}

.container1 {
  height: 561rpx;
  width: 100%;
  background: url('data:image/png;base64,');
  background-size: cover;
}
