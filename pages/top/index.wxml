<view class='wrap'>
  <top title='畅销商品排行' bind:goback='goback'></top>
</view>
<view class='condition_wrap'>
  <view class='condition'>
    <select prop-array='{{saleTypeArray}}' class='select_box' placeholder='销售类型'></select>
    <select prop-array='{{dateTypeArray}}' class='select_box' placeholder='日期类型'></select>
    <select prop-array='{{areaList}}'  class='select_box' placeholder='区域'></select>
  </view>
</view>
<view class='table_box'>
  <table columns='{{columns}}' listData="{{pageData}}"></table>
</view>